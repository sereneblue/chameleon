{
  "name": "chameleon-ext",
  "version": "0.20.17",
  "description": "Spoof your browser profile. Includes a few privacy enhancing options.",
  "author": "sereneblue",
  "license": "GPLv3",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "prettier": "prettier \"src/**/*.{js,vue}\"",
    "prettier:write": "npm run prettier -- --write",
    "build": "cross-env NODE_ENV=production webpack --hide-modules",
    "build:dev": "cross-env NODE_ENV=development webpack --hide-modules",
    "build-zip": "node scripts/build-zip.js",
    "test": "npm run build && npm run build-zip && jest --detectOpenHandles",
    "watch": "npm run build -- --watch",
    "watch:dev": "cross-env HMR=true npm run build:dev -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "globalSetup": "<rootDir>/tests/setup.js",
    "globalTeardown": "<rootDir>/tests/teardown.js",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "cidr-js": "^2.3.1",
    "feather-icons": "^4.25.0",
    "moment-timezone": "^0.5.27",
    "psl": "^1.7.0",
    "tailwindcss": "^1.4.6",
    "tailwindcss-toggle": "github:TowelSoftware/tailwindcss-toggle",
    "uuid": "^8.0.0",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-feather": "^1.0.0",
    "vue-i18n": "^8.17.4",
    "vue-object-merge": "^0.1.7",
    "vue-property-decorator": "^8.4.0",
    "vue2-perfect-scrollbar": "^1.3.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime-corejs3": "^7.8.4",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/chrome": "^0.0.74",
    "@types/node": "^12.12.26",
    "archiver": "^3.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.1",
    "ejs": "^3.1.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "file-loader": "^1.1.11",
    "geckodriver": "^1.19.1",
    "husky": "^2.4.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.4.4",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.8.0",
    "sass-loader": "^7.1.0",
    "selenium-webdriver": "^4.0.0-alpha.5",
    "ts-jest": "^26.0.0",
    "ts-loader": "^7.0.2",
    "typescript": "^3.7.5",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "web-ext": "^4.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-extension-reloader": "^1.1.4"
  }
}
