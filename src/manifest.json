	"browser_action": {
		"default_icon": "img/icon_48.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"48": "img/icon_48.png",
		"128": "img/icon_128.png"
	},
	"permissions": [
		"<all_urls>",
		"alarms",
		"notifications",
		"privacy",
		"storage",
		"webRequest",
		"webRequestBlocking"
	]
